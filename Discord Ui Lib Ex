local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/BloxiYT/Scripts/main/Discord%20Ui%20Lib"))()

local gui = Library:Create("Bloxi's server")
local section1 = gui:AddSection("ðŸ§€ï½œweryfikacja")

section1:AddButton(
    "Button",
    function()
        print("a")
    end
)

section1:AddSlider(
    "Slider",
    0,
    10,
    function(value)
        print(value)
    end
)

section1:AddToggle(
    "Toggle",
    function(enabled)
        print(enabled)
    end
)

section1:AddButton(
    "Notification",
    function()
        gui:Notify("Title","Description")
    end
)

section1:AddLabel("Label")

section1:AddTextbox(
    "Textbox Title",
    "Preview Text",
    function(text)
        print(text)
    end
)

section1:Select() -- selects page

